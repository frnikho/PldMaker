{
  "presets": [
    [
      "@nrwl/react/babel",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["styled-components", { "pure": true, "ssr": true }],
    ["@babel/plugin-proposal-private-methods", { "loose": false }]
  ]
}
